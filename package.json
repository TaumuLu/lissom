    "lightweight",
    "react",
    "ssr"
  ],
  "homepage": "https://github.com/TaumuLu/issues#readme",
  "dependencies": {
    "ansi-html": "0.0.7",
    "chalk": "^2.4.2",
    "etag": "1.8.1",
    "find-up": "^3.0.0",
    "fresh": "0.5.2",
    "html-webpack-plugin": "^3.2.0",
    "htmlescape": "1.1.1",
    "http-status": "^1.3.1",
