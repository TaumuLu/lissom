    "@types/html-webpack-plugin": "^3.2.0",
    "@types/koa-router": "^7.0.35",
    "@types/node": "^11.9.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/webpack": "^4.4.24",
    "@types/webpack-sources": "^0.1.5",
    "husky": "^1.3.1",
    "koa-router": "^7.4.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
