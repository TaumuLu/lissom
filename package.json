{
  "name": "lissom",
  "version": "1.2.0",
  "description": "Lightweight React Server Side Rendering Framework",
  "main": "./index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc",
    "watch": "tsc -w",
    "prepublish": "npm run build",
    "lint": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
